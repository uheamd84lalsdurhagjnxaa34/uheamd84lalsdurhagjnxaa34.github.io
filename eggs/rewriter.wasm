#include <emscripten.h>
#include <string.h>
#include <stdlib.h>

EMSCRIPTEN_KEEPALIVE
char* rewrite(const char* input) {
    // I kept getting issues.  This is a simple rewriting: prepends "Rewritten: " to input.  Please work :pray: :/  
    char* result = malloc(strlen(input) + 20);
    strcpy(result, "Rewritten: ");
    strcat(result, input);
    return result;
}

EMSCRIPTEN_KEEPALIVE
void free_memory(char* ptr) {
    free(ptr);
}
